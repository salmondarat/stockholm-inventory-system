# GitHub Repository Configuration
GITHUB_REPOSITORY=salmondarat/stockholm-inventory-system
GITHUB_OWNER=salmondarat

# GitHub API Configuration (for automated tasks)
# GITHUB_TOKEN=your_github_personal_access_token_here

# Database Configuration (adjust based on your setup)
# DATABASE_URL=postgresql://username:password@localhost:5432/stockholm_inventory
# DATABASE_HOST=localhost
# DATABASE_PORT=5432
# DATABASE_NAME=stockholm_inventory
# DATABASE_USER=username
# DATABASE_PASSWORD=password

# Application Configuration
NODE_ENV=development
PORT=3000

# API Keys (if using external services)
# INVENTORY_API_KEY=your_api_key_here
# ANALYTICS_API_KEY=your_analytics_key_here

# Email Configuration (for notifications)
# EMAIL_HOST=smtp.gmail.com
# EMAIL_PORT=587
# EMAIL_USER=your_email@example.com
# EMAIL_PASSWORD=your_app_password

# Logging
LOG_LEVEL=info

# Security
# JWT_SECRET=your_jwt_secret_here
# SESSION_SECRET=your_session_secret_here

# File Upload Configuration
# MAX_FILE_SIZE=10MB
# UPLOAD_PATH=./uploads
